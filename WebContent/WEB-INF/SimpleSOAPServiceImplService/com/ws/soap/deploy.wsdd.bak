<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from SimpleSOAPServiceImplService WSDL service -->

  <service name="SimpleSOAPServiceImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://soap.ws.com"/>
      <parameter name="wsdlServiceElement" value="SimpleSOAPServiceImplService"/>
      <parameter name="schemaQualified" value="http://soap.ws.com"/>
      <parameter name="wsdlServicePort" value="SimpleSOAPServiceImpl"/>
      <parameter name="className" value="com.ws.soap.SimpleSOAPServiceImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="SimpleSOAPServiceImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sayHello" qname="operNS:sayHello" xmlns:operNS="http://soap.ws.com" returnQName="retNS:sayHelloReturn" xmlns:retNS="http://soap.ws.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:echo" xmlns:pns="http://soap.ws.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="sayHello"/>

  </service>
</deployment>
